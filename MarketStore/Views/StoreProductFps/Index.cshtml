@model IEnumerable<MarketStore.Models.StoreProductFp>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Product.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Store.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Product.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Store.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StoreProductId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StoreProductId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StoreProductId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
